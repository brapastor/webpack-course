{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "curso de webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js -o js/bundle.js --mode development",
    "build:local": "webpack --mode development",
    "build:external": "webpack --mode development --config ./external/webpack.config.js",
